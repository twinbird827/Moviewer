<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mocore="clr-namespace:Moviewer.Core.Styles"
                    xmlns:tuctrl="clr-namespace:Moviewer.Tube.Controls">

    <DataTemplate DataType="{x:Type tuctrl:TubeTagViewModel}">
        <mocore:LinkedTextBlock Margin="5,3"
                                Command="{Binding OnClickTag}"
                                Text="{Binding Tag}" />
    </DataTemplate>
</ResourceDictionary>